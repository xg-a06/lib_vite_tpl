{
  "name": "vite-project",
  "version": "0.0.0",
  "scripts": {
    "start": "vite --config build/vite.demo.conf.js",
    "build:demo": "vite build --config build/vite.demo.conf.js",
    "build:prod": "vite build --config build/vite.lib.conf.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:demo && npm run build:prod",
    "serve": "vite preview -c build/vite.demo.conf.js"
  },
  "devDependencies": {
    "@rollup/plugin-eslint": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "glob": "^7.1.6",
    "typescript": "^4.2.3",
    "vite": "^2.1.0",
    "webpack-merge": "^5.7.3"
  }
}
